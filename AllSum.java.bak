import java.util.Scanner;
class  AllSum
{
	public static void main(String[] args) 
	{
		System.out.println("Enter Any Number");
		Scanner sc= new Scanner(System.in);
		long num = sc.nextLong();
		long sum=0;
		while (num!=0)
		{
			long last=num%10;
			sum+=last;
			num/=10;
		}System.out.println("Sum Of All Digits "+sum);
		
		
		
		
		
		
	}
}
